{{extend 'template.html'}}

<div class="tabbable"> <!-- Only required for left/right tabs -->
  <ul class="nav nav-tabs">
    {{for (i, item) in enumerate(periods):}}
      <li class="active"><a href="#{{=i}}tab" data-toggle="tab">{{=item['name']}}</a></li>
    {{pass}}
  </ul>
  <div class="tab-content">
    {{for (i, item) in enumerate(periods):}}
        <div class="tab-pane" id="{{=i}}tab">
            {{=item['grid']}}
        </div>
    {{pass}}
  </div>
</div>
<h1>{{=T('Bulk Final Practice Assignation')}}</h1>
{{if success:}}
    Inserted users: <br/>
	{{=BEAUTIFY(data)}}
	<br/>
Users not found:<br/>
    {{=BEAUTIFY(errors)}}

Already existing users:<br/>
	{{=BEAUTIFY(existUsers)}}

{{else:}}
	{{=FORM(INPUT(_type='file',_name='csvfile'),INPUT(_type='submit',_value='Upload'))}}
{{ pass }}

{{if request.is_local:}}
{{=response.toolbar()}}
{{pass}}
