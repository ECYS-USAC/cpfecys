{{extend 'template.html'}}

<div class="tabbable"> <!-- Only required for left/right tabs -->
  <ul class="nav nav-tabs">
      <li><a href="#setuptab" data-toggle="tab">{{=T('Other Periods')}}</a></li>
    {{for (i, item) in enumerate(periods):}}
      <li><a href="#{{=i}}tab" data-toggle="tab">{{=item['name']}}</a></li>
    {{pass}}
  </ul>
  <div class="tab-content">
      <div class="tab-pane" id="setuptab">
          <form>
              <select name="year_period">
                  {{for item in periods_years:}}
                  <option value="{{=item.id}}">{{=str(item.yearp) + ' - ' + str(item.period) }}</option>
                  {{pass}}
              </select>
              <input type="submit" value="go" >
          </form>
      </div>
    {{for (i, item) in enumerate(periods):}}
        <div class="tab-pane" id="{{=i}}tab">
            <h2>
                {{=item['name']}}
            </h2>
            {{=item['grid']}}
        </div>
    {{pass}}
  </div>
</div>
<h1>{{=T('Bulk Final Practice Assignation')}}</h1>
{{if success:}}
    Inserted users: <br/>
	{{=BEAUTIFY(data)}}
	<br/>
Users not found:<br/>
    {{=BEAUTIFY(errors)}}

Already existing users:<br/>
	{{=BEAUTIFY(existUsers)}}

{{else:}}
	{{=FORM(INPUT(_type='file',_name='csvfile'),INPUT(_type='submit',_value='Upload'))}}
{{ pass }}

{{if request.is_local:}}
{{=response.toolbar()}}
{{pass}}
